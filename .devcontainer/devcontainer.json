{
    "name": "tinygo-example",
    "dockerFile": "Dockerfile",
    "extensions": [
        "golang.go",
        "yzhang.markdown-all-in-one",
        "davidanson.vscode-markdownlint",
        "wmaurer.change-case",
        "asuka.insertnumbers",
        "eamodio.gitlens",
        "tinygo.vscode-tinygo"
    ],
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "settings": {
        "go.gopath": "/go",
        "go.useLanguageServer": true
    }
}
